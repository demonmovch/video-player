{
    "name": "react-project",
    "version": "1.0.0",
    "author": "Dima Movchan",
    "main": "index.js",
    "scripts": {
        "start": "cross-env-shell NODE_ENV=development node --require @babel/register \"./scripts/webpack/start.js\"",
        "build": "cross-env-shell NODE_ENV=production node --require @babel/register \"./scripts/webpack/build.js\"",
        "build:analyze": "cross-env-shell ANALYZE=true run-s build analyze",
        "analyze": "webpack-bundle-analyzer \"./build/build-stats.json\" build",
        "lint": "run-s lint:javascript lint:css",
        "lint:javascript": "jest --projects \"./scripts/jest/jest.eslint.config.js\"",
        "lint:css": "jest --projects \"./scripts/jest/jest.stylelint.config.js\"",
        "test": "cross-env-shell NODE_ENV=test jest --config=\"./scripts/jest/jest.test.config.js\"",
        "test:watch": "npm test -- --watch",
        "test:debug": "node --inspect-brk \"./node_modules/.bin/jest\" --runInBand --config=\"./scripts/jest/jest.test.config.js\"",
        "soundcheck": "jest --config=\"./scripts/jest/jest.config.js\"",
        "prettier": "prettier-eslint --list-different \"./source/**/*.js\"; prettier-eslint --write \"./source/**/*.js\"",
        "serve": "serve \"./build\" -s"
    },
    "dependencies": {
        "classnames": "^2.2.6",
        "mobx": "^5.11.0",
        "mobx-react": "^6.1.1",
        "prop-types": "^15.7.2",
        "react": "^16.8.6",
        "react-dom": "^16.8.6"
    },
    "devDependencies": {
        "@babel/core": "^7.5.4",
        "@babel/plugin-proposal-class-properties": "^7.4.0",
        "@babel/plugin-proposal-decorators": "^7.4.0",
        "@babel/plugin-transform-react-jsx": "^7.3.0",
        "@babel/polyfill": "^7.4.4",
        "@babel/preset-env": "^7.4.2",
        "@babel/preset-react": "7.0.0",
        "@babel/register": "7.0.0",
        "@svgr/webpack": "4.1.0",
        "app-root-path": "2.1.0",
        "babel-core": "^6.26.3",
        "babel-eslint": "^10.0.2",
        "babel-loader": "8.0.5",
        "babel-plugin-dynamic-import-node": "2.2.0",
        "chalk": "2.4.2",
        "clean-webpack-plugin": "1.0.1",
        "core-js": "^2.6.9",
        "cross-env": "5.2.0",
        "css-loader": "2.1.1",
        "cssnano": "4.1.8",
        "eslint": "^6.0.1",
        "eslint-config-airbnb-base": "^13.2.0",
        "eslint-config-babel": "^9.0.0",
        "eslint-config-prettier": "^6.0.0",
        "eslint-plugin-babel": "^5.3.0",
        "eslint-plugin-import": "^2.18.0",
        "eslint-plugin-react": "^7.14.2",
        "eslint-plugin-react-hooks": "^1.6.1",
        "file-loader": "3.0.1",
        "friendly-errors-webpack-plugin": "1.7.0",
        "html-webpack-plugin": "3.2.0",
        "identity-obj-proxy": "3.0.0",
        "imagemin-mozjpeg": "8.0.0",
        "imagemin-pngquant": "7.0.0",
        "imagemin-svgo": "7.0.0",
        "imagemin-webpack": "4.1.0",
        "mini-css-extract-plugin": "0.5.0",
        "npm-run-all": "4.1.5",
        "postcss-easing-gradients": "3.0.1",
        "postcss-font-magician": "2.2.1",
        "postcss-font-smoothing": "0.1.0",
        "postcss-icss-selectors": "2.0.3",
        "postcss-loader": "3.0.0",
        "postcss-preset-env": "6.5.0",
        "postcss-reporter": "6.0.1",
        "prettier": "^1.18.2",
        "react-dev-utils": "7.0.1",
        "react-hot-loader": "^4.12.6",
        "regenerator-runtime": "^0.13.2",
        "serve": "10.1.2",
        "style-loader": "0.23.1",
        "stylelint": "9.10.1",
        "url-loader": "^1.1.2",
        "webpack": "^4.29.6",
        "webpack-bundle-analyzer": "3.0.3",
        "webpack-cli": "^3.3.0",
        "webpack-dev-server": "^3.2.1",
        "webpack-dev-server-waitpage": "1.0.0",
        "webpack-hot-middleware": "^2.25.0",
        "webpack-merge": "4.2.1"
    }
}
